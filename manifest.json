{
  "name": "Extension Switch",
  "description": "an extension switch application",
  "version": "1.0",
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "management"
  ],
  "browser_action": {
    "default_title": "Extension Switch",
    "default_icon": {
      "16": "images/16.png",
      "32": "images/32.png",
      "48": "images/48.png",
      "128": "images/128.png"
    },
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}